{
  "name": "qa-cloud",
  "version": "0.0.1",
  "description": "QA Cloud",
  "author": "Kelvin.Ye <testmankelvin@163.com>",
  "engines": {
    "node": ">= 16"
  },
  "type": "module",
  "scripts": {
    "eslint": "eslint --ext .js,.jsx,.vue src/",
    "stylelint": "stylelint **/*.{css,less,scss,vue} --fix",
    "dev": "vite --mode development",
    "build": "node ./build/modify-keepalive.cjs && vite build --mode production",
    "preview": "pnpm run build && vite preview",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.1",
    "@tanstack/vue-query": "5.29.0",
    "axios": "1.6.8",
    "clipboard": "2.0.11",
    "core-js": "3.36.1",
    "croner": "8.0.1",
    "dayjs": "1.11.10",
    "echarts": "5.5.0",
    "element-plus": "2.6.3",
    "fuse.js": "7.0.0",
    "intro.js": "7.2.0",
    "js-cookie": "3.0.5",
    "jsencrypt": "3.3.2",
    "json5": "2.2.3",
    "localforage": "1.10.0",
    "lodash-es": "4.17.21",
    "monaco-editor": "0.45.0",
    "mousetrap": "1.6.5",
    "nanoid": "5.0.7",
    "nprogress": "0.2.0",
    "path": "0.12.7",
    "path-to-regexp": "6.2.2",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "3.2.1",
    "qs": "6.12.0",
    "radash": "12.1.0",
    "screenfull": "6.0.2",
    "socket.io-client": "4.7.5",
    "sortablejs": "1.15.2",
    "sortablejs-vue3": "1.2.11",
    "ulid": "2.3.0",
    "vue": "3.4.21",
    "vue-hooks-plus": "1.8.9",
    "vue-router": "4.3.0",
    "vuedraggable": "4.1.0"
  },
  "devDependencies": {
    "@arco-design/web-vue": "2.55.1",
    "@arco-plugins/vite-vue": "1.4.5",
    "@babel/core": "7.24.4",
    "@babel/eslint-parser": "7.24.1",
    "@tanstack/eslint-plugin-query": "5.28.11",
    "@types/echarts": "4.9.22",
    "@types/lodash-es": "4.17.12",
    "@types/mousetrap": "1.6.15",
    "@types/nprogress": "0.2.3",
    "@types/qs": "6.9.14",
    "@types/sortablejs": "1.15.8",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/compiler-sfc": "3.4.21",
    "@vue/runtime-core": "3.4.21",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-oxlint": "0.2.7",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "52.0.0",
    "eslint-plugin-vue": "9.24.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "oxlint": "0.2.16",
    "prettier": "3.2.5",
    "rollup": "4.14.1",
    "sass": "1.74.1",
    "stylelint": "16.3.1",
    "stylelint-config-recess-order": "5.0.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-config-standard-vue": "1.0.0",
    "unplugin-auto-import": "0.17.5",
    "unplugin-https-reverse-proxy": "1.0.0-beta.12",
    "unplugin-icons": "0.18.5",
    "unplugin-vue-components": "0.26.0",
    "unplugin-vue-router": "0.8.5",
    "vite": "5.2.8",
    "vite-plugin-style-import": "2.0.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "7.0.25",
    "vue-eslint-parser": "9.4.2"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "prettier": {
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "jsxSingleQuote": false,
    "printWidth": 120,
    "quoteProps": "as-needed",
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "rangeStart": 0
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix"
    ],
    "*.{html,css,scss,md}": [
      "prettier --write"
    ]
  }
}
