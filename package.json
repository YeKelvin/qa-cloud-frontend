{
  "name": "qa-cloud",
  "version": "0.0.1",
  "description": "QA Cloud",
  "author": "Kelvin.Ye <testmankelvin@163.com>",
  "engines": {
    "node": ">= 16"
  },
  "type": "module",
  "scripts": {
    "eslint": "eslint --ext .js,.jsx,.vue src/",
    "stylelint": "stylelint **/*.{css,less,scss,vue} --fix",
    "dev": "vite --mode development",
    "build": "node ./build/modify-keepalive.cjs && vite build --mode production",
    "preview": "pnpm run build && vite preview",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.1",
    "@tanstack/vue-query": "5.18.0",
    "axios": "1.6.7",
    "clipboard": "2.0.11",
    "core-js": "3.35.1",
    "cron-validator": "1.3.1",
    "dayjs": "1.11.10",
    "echarts": "5.4.3",
    "element-plus": "2.5.3",
    "fuse.js": "7.0.0",
    "intro.js": "7.2.0",
    "js-cookie": "3.0.5",
    "jsencrypt": "3.3.2",
    "json5": "2.2.3",
    "localforage": "1.10.0",
    "lodash-es": "4.17.21",
    "monaco-editor": "0.45.0",
    "mousetrap": "1.6.5",
    "nanoid": "5.0.5",
    "nprogress": "0.2.0",
    "path": "0.12.7",
    "path-to-regexp": "6.2.1",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "3.2.1",
    "qs": "6.11.2",
    "screenfull": "6.0.2",
    "socket.io-client": "4.7.4",
    "sortablejs": "1.15.2",
    "sortablejs-vue3": "1.2.11",
    "ulid": "2.3.0",
    "vue": "3.3.13",
    "vue-hooks-plus": "^1.8.8",
    "vue-router": "4.2.5",
    "vuedraggable": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/eslint-parser": "7.23.10",
    "@types/echarts": "4.9.22",
    "@types/lodash-es": "4.17.12",
    "@types/mousetrap": "1.6.15",
    "@types/nprogress": "0.2.3",
    "@types/qs": "6.9.11",
    "@types/sortablejs": "1.15.7",
    "@vitejs/plugin-vue": "5.0.3",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/compiler-sfc": "3.3.13",
    "@vue/runtime-core": "3.3.13",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-oxlint": "0.2.3",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "50.0.1",
    "eslint-plugin-vue": "9.21.1",
    "husky": "9.0.10",
    "lint-staged": "15.2.1",
    "oxlint": "0.2.6",
    "prettier": "3.2.4",
    "rollup": "4.9.6",
    "sass": "1.70.0",
    "stylelint": "16.2.1",
    "stylelint-config-recess-order": "4.4.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-config-standard-vue": "1.0.0",
    "unplugin-auto-import": "0.17.5",
    "unplugin-icons": "0.18.3",
    "unplugin-vue-components": "0.26.0",
    "unplugin-vue-define-options": "1.4.2",
    "vite": "5.0.12",
    "vite-plugin-style-import": "2.0.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "7.0.14",
    "vue-eslint-parser": "9.4.2"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "prettier": {
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "jsxSingleQuote": false,
    "printWidth": 120,
    "quoteProps": "as-needed",
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "rangeStart": 0
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix"
    ],
    "*.{html,css,scss,md}": [
      "prettier --write"
    ]
  }
}
