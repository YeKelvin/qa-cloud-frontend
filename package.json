{
  "name": "qa-cloud",
  "version": "0.0.1",
  "description": "QA Cloud",
  "author": "Kelvin.Ye <testmankelvin@163.com>",
  "packageManager": "pnpm@6.32.0",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "eslint": "eslint --ext .js,.jsx,.vue src/",
    "stylelint": "stylelint **/*.{css,less,scss,vue} --fix",
    "dev": "vite --mode development",
    "build": "node ./build/modify-keepalive.js && vite build --mode production",
    "preview": "pnpm run build && vite preview",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.1.0",
    "axios": "1.5.1",
    "clipboard": "2.0.11",
    "core-js": "3.33.0",
    "cron-validator": "1.3.1",
    "dayjs": "1.11.10",
    "echarts": "5.4.3",
    "element-plus": "2.3.14",
    "fuse.js": "6.6.2",
    "intro.js": "7.2.0",
    "js-cookie": "3.0.5",
    "jsencrypt": "3.3.2",
    "json5": "2.2.3",
    "lodash-es": "4.17.21",
    "monaco-editor": "0.43.0",
    "mousetrap": "1.6.5",
    "nanoid": "5.0.1",
    "nprogress": "0.2.0",
    "path": "0.12.7",
    "path-to-regexp": "6.2.1",
    "pinia": "2.1.6",
    "pinia-plugin-persistedstate": "3.2.0",
    "qs": "6.11.2",
    "screenfull": "6.0.2",
    "socket.io-client": "4.7.2",
    "sortablejs": "1.15.0",
    "sortablejs-vue3": "1.2.9",
    "ulid": "2.3.0",
    "vue": "3.3.4",
    "vue-router": "4.2.5",
    "vuedraggable": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@babel/eslint-parser": "7.22.15",
    "@types/echarts": "4.9.19",
    "@types/lodash-es": "4.17.9",
    "@types/mousetrap": "1.6.12",
    "@types/nprogress": "0.2.1",
    "@types/qs": "6.9.8",
    "@types/sortablejs": "1.15.3",
    "@vitejs/plugin-vue": "4.4.0",
    "@vitejs/plugin-vue-jsx": "3.0.2",
    "@vue/compiler-sfc": "3.3.4",
    "@vue/runtime-core": "3.3.4",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint-plugin-vue": "9.17.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "rollup-plugin-visualizer": "5.9.2",
    "sass": "1.69.0",
    "stylelint": "15.10.3",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.3.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "stylelint-config-standard-vue": "1.0.0",
    "unplugin-auto-import": "0.16.6",
    "unplugin-element-plus": "0.8.0",
    "unplugin-icons": "0.17.0",
    "unplugin-vue-components": "0.25.2",
    "unplugin-vue-define-options": "1.3.18",
    "vite": "4.4.11",
    "vite-plugin-style-import": "2.0.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "0.5.1",
    "vue-eslint-parser": "9.3.1"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "prettier": {
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "jsxSingleQuote": false,
    "printWidth": 120,
    "quoteProps": "as-needed",
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "rangeStart": 0
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix"
    ],
    "*.{html,css,scss,md}": [
      "prettier --write"
    ]
  }
}
